  "author": "Stepan Zhevak <enet@protonmail.ch>",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": false,
  "scripts": {
    "clean:game": "rm -rf ./game-output/bundles ./game-output/images ./game-output/sounds",
    "clean:editor": "rm -rf ./editor-output/bundles ./editor-output/images",
    "clean": "yarn clean:game; yarn clean:editor",
    "build:game": "yarn clean:game && yarn webpack -p --config webpack.game.js",
    "build:editor": "yarn clean:editor && yarn webpack -p --config webpack.editor.js",
    "build": "yarn build:game; yarn build:editor",
    "watch:game": "yarn webpack --watch --config webpack.game.js",
    "watch:editor": "yarn webpack --watch --config webpack.editor.js",
    "start:game": "yarn webpack-dev-server --config webpack.game.js",
    "start:editor": "yarn webpack-dev-server --config webpack.editor.js"
  }
}
